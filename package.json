{
    "name": "express-ts-sms-api",
    "version": "1.0.0",
    "description": "A scalable Express.js API with TypeScript for SMS processing",
    "main": "dist/index.js",
    "scripts": {
        "prestart": "npm run build",
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "lint": "eslint src/**/*.ts",
        "format": "prettier --write src/**/*.ts",
        "type-check": "tsc --noEmit"
    },
    "keywords": [
        "express",
        "typescript",
        "sms",
        "api"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@google/generative-ai": "^0.24.1",
        "axios": "^1.11.0",
        "cors": "^2.8.5",
        "dotenv": "^17.2.1",
        "express": "^4.18.2",
        "winston": "^3.17.0",
        "typescript": "^5.1.6"
    },
    "devDependencies": {
        "@types/cors": "^2.8.14",
        "@types/express": "^4.17.17",
        "@types/node": "^20.19.11",
        "@typescript-eslint/eslint-plugin": "^6.4.0",
        "@typescript-eslint/parser": "^6.4.0",
        "eslint": "^8.47.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "prettier": "^3.0.2",
        "ts-node-dev": "^2.0.0"
    }
}
